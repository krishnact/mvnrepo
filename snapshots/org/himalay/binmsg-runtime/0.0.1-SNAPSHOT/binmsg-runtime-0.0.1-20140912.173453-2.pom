	<distributionManagement>
		<repository>
			<id>repo</id>
			<url>https://github.com/krishnact/mvnrepo/raw/master/releases</url>
		</repository>
		<snapshotRepository>
			<id>snapshot-repo</id>
			<url>https://github.com/krishnact/mvnrepo/raw/master/snapshots</url>
		</snapshotRepository>
	</distributionManagement>
</project>